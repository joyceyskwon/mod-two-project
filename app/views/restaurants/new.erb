<h1>New Restaurant</h1>

<%= form_for @restaurant do |f| %>
  <%= f.label :name %><br>
  <br>
  <%= f.text_field :name %><br>
  <br>
  <%= f.label :address %><br>
  <br>
  <%= f.text_field :address %><br>
  <br>
  <%= f.label :yelp_url %><br>
  <br>
  <%= f.text_field :yelp_url %><br>
  <br>
  <%= f.submit 'Enlist new restaurant' %>
<% end %>
